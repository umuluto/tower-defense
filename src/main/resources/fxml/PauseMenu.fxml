<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" type="HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="resume_button" mnemonicParsing="false" onAction="#resumeGame" text="Resume">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="save_button" mnemonicParsing="false" onAction="#saveGame" text="Save">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="exit_button" mnemonicParsing="false" onAction="#exitGame" text="Exit">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
