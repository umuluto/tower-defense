<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root prefHeight="480.0" prefWidth="640.0" type="javafx.scene.layout.StackPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Button fx:id="start_button" alignment="CENTER" mnemonicParsing="false" onAction="#startGame" text="Start Game" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="load_button" alignment="CENTER" mnemonicParsing="false" onAction="#loadGame" text="Load Game" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="exit_button" alignment="CENTER" mnemonicParsing="false" onAction="#exit" text="Exit" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
         <StackPane.margin>
            <Insets top="100.0" />
         </StackPane.margin>
      </VBox>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tower Defense" StackPane.alignment="TOP_CENTER">
         <font>
            <Font size="51.0" />
         </font>
         <StackPane.margin>
            <Insets top="50.0" />
         </StackPane.margin>
      </Text>
   </children>
</fx:root>
